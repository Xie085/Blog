<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xxc.dao.CategoryMapper">
    <select id="getAllCategory" resultType="com.xxc.bean.Category">
        select * from myblog.t_category
    </select>

    <select id="getCategoryById" resultType="com.xxc.bean.Category">
        select * from myblog.t_category where id = #{id}
    </select>
</mapper>